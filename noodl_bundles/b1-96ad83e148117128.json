}\r\nanimateWords(); // Start the animation"},"ports":[{"name":"outtype-dsize","displayName":"Type","editorName":"dsize | Type","plug":"input","type":{"name":"enum","enums":[{"value":"string","label":"String"},{"value":"boolean","label":"Boolean"},{"value":"number","label":"Number"},{"value":"object","label":"Object"},{"value":"date","label":"Date"},{"value":"array","label":"Array"},{"value":"color","label":"Color"},{"value":"signal","label":"Signal"}],"allowEditOnly":true},"default":"string","parent":"scriptOutputs","parentItemId":"tk1x","index":4},{"name":"out-dsize","displayName":"dsize","plug":"output","type":"*","group":"Outputs","index":5}],"children":[]},{"id":"dcd4f9ea-9776-6902-1588-507f8841da7b","type":"/Media Queries/Media Query Setup","parameters":{},"ports":[],"children":[]},{"id":"4663e927-4843-2a7b-bce3-a9b58516728f","type":"/Media Queries/Match Media Query","parameters":{},"ports":[],"children":[]},{"id":"5bc7905a-5949-93d3-2242-ec763a364f8d","type":"States","parameters":{"states":"Mobile,Desktop","values":"Column alignment,card_header_fontsize,icon_size,card_body_fontsize,pp_size,tagline_size,left_patterns_margin,demo desc alignment","type-Column alignment":"string","value-Mobile-Column alignment":"Vertical","value-Desktop-Column alignment":"Horizontal","currentState":"Mobile","value-Desktop-card_header_fontsize":27,"value-Mobile-card_header_fontsize":16,"value-Desktop-icon_size":48,"value-Mobile-icon_size":30,"value-Desktop-card_body_fontsize":20,"value-Mobile-card_body_fontsize":13,"value-Mobile-pp_size":40,"value-Desktop-pp_size":83,"value-Mobile-left_patterns_margin":10,"value-Desktop-left_patterns_margin":32,"value-Desktop-tagline_size":40,"value-Mobile-tagline_size":27,"type-demo desc alignment":"string","value-Mobile-demo desc alignment":"row","value-Desktop-demo desc alignment":"column"},"ports":[],"children":[]},{"id":"7c8de4b8-6060-bc20-f198-fc4c3ae01a99","type":"Condition","parameters":{"condition":false},"ports":[],"children":[]}],"conne